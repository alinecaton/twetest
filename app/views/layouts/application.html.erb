<!DOCTYPE html>
<html>
<head>
  <title>Twetest</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<h1><%= link_to 'Twetest', root_path %></h1>

<div class='flash'>
    <% flash.each do |type, message| %>
    <div class='message <%= type %>'>
      <p><%= message %></p>
    </div>
    <% end %>
  </div>
<%= yield %>

</body>
</html>
